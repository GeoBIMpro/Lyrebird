<UserControl x:Class="LMNA.Lyrebird.GH.ParameterControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="250">
    <Grid>
        <Label Name="paramNameLabel" Content="Parameter Name" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,0,0,0" FontSize="16" Foreground="#FF424242"/>
        <Label Name="storageTypeLabel" Content="Storage Type" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,22,0,0" FontSize="12" Foreground="#FF424242"/>
        <Label Name="isTypeLabel" Content="Instance" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="125,22,0,0" FontSize="12" Foreground="#FF424242"/>
        <Rectangle Fill="#FF424242" Height="1" HorizontalAlignment="Stretch" VerticalAlignment="Top" Margin="10,-1,10,0"/>
    </Grid>
</UserControl>

